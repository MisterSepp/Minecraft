paperversion="1.18" && number=$(curl https://papermc.io/api/v2/projects/paper/versions/$paperversion/ |sed 's/.*builds//' | grep -o '[0-9]*' | sort -nr | head -1) && curl https://papermc.io/api/v2/projects/paper/versions/$paperversion/builds/$number/downloads/paper-$paperversion-$number.jar -o /home/opc/server/download/paper.jar
