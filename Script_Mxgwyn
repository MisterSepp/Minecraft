paperversion="1.19" && number=$(curl https://api.papermc.io/v2/projects/paper/versions/$paperversion/ |sed 's/.builds//' | grep -o '[0-9]' | sort -nr | head -1) && curl https://api.papermc.io/v2/projects/paper/versions/$paperversion/builds/$number/downloads/paper-$paperversion-$number.jar -o /home/opc/server/download/paper.jar
